  "dependencies": {
    "broccoli-plugin": "^1.2.1",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "node-dir": "^0.1.11",
    "node-sass": "^3.4.2",
    "rsvp": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-istanbul": "^0.5.9",
    "broccoli-fixture": "^0.1.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "codecov.io": "^0.1.6",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^1.0.2",
    "mocha": "^2.3.4"
