{
  "name": "broccoli-sass-tree",
  "version": "0.1.0-alpha.2",
  "description": "Libsass-based Sass compiler for Broccoli",
  "license": "MIT",
  "repository": "amercier/broccoli-sass-tree",
  "author": {
    "name": "Alex Mercier",
    "email": "pro.alexandre.mercier@gmail.com",
    "url": "amercier.com"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "rm -rf dist && babel lib -d dist",
    "test": "eslint . && babel-node ./node_modules/.bin/babel-istanbul cover _mocha",
