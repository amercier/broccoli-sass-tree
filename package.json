    "node-dir": "^0.1.11",
    "node-sass": "^3.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "broccoli-fixture": "^0.1.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
